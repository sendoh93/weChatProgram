/* pages/reward/reward.wxss */
.reward {
    width: 100%;
    height:  100vh;
    display: flex;
    background-color: white;
    flex-direction: column;
    align-items: center
}

.rewardBtn{
    width: 90%;
    height: 80rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background-size: 100% 100%;
    background-image: url('data:image/png;base64,');
    border-radius: 10rpx;
    margin-top: 30rpx;
}

.rewardBtn image{
    width:80%;
    height: 80rpx;
    z-index: -1;
}

.rewardBtn text{
    font-size: 32rpx;
    color: white;
    z-index: 1;
    position: fixed;
}

.rewardBtn::after{
  border: 0;
}

.listTop {
  width: 90%;
  margin-top: 52rpx;
  display: flex;
  flex-direction: row;
}
.listTop text{
  flex: 1;
}

.listTopItem {
  width: 90%;
  height: 100rpx;
  display: flex;
  align-items: center;
  flex-direction: row;
  border-top: solid 1rpx #E2E2E2;
}

.listTopItem text{
flex: 1;
}